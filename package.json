{
  "name": "ngx-translate-plugins",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn run build:testing && yarn run build:missing-translations && yarn run build:demo",
    "build:demo": "ng build",
    "build:testing": "ng build testing",
    "build:testing:prod": "yarn run build:testing --prod",
    "build:missing-translations": "ng build missing-translations",
    "build:missing-translations:prod": "yarn run build:missing-translations --prod",
    "clean": "rimraf dist coverage",
    "coverage:demo": "yarn run test:demo --code-coverage true",
    "coverage:testing": "yarn run test:testing --code-coverage true",
    "coverage:missing-translations": "yarn run test:missing-translations --code-coverage true",
    "pretest": "yarn run build:testing",
    "test": "yarn run test:testing && yarn run test:missing-translations && yarn run test:demo",
    "test:demo": "ng test --project=ngx-translate-plugins",
    "test:testing": "ng test --project=testing",
    "test:missing-translations": "ng test --project=missing-translations",
    "lint": "yarn run lint:testing && yarn run lint:missing-translations && yarn run lint:demo",
    "lint:demo": "ng lint --project=ngx-translate-plugins",
    "lint:testing": "ng lint --project=testing",
    "lint:missing-translations": "ng lint --project=missing-translations",
    "e2e": "ng e2e",
    "precommit": "lint-staged -r"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.6",
    "@angular/common": "^8.2.6",
    "@angular/compiler": "^8.2.6",
    "@angular/core": "^8.2.6",
    "@angular/forms": "^8.2.6",
    "@angular/platform-browser": "^8.2.6",
    "@angular/platform-browser-dynamic": "^8.2.6",
    "@angular/router": "^8.2.6",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "core-js": "^2.5.4",
    "messageformat": "^2.0.2",
    "ngx-translate-messageformat-compiler": "^4.4.0",
    "rxjs": "^6.3.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.4",
    "@angular-devkit/build-ng-packagr": "~0.803.4",
    "@angular/cli": "^8.3.4",
    "@angular/compiler-cli": "^8.2.6",
    "@angular/language-service": "^8.2.6",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^9.2.5",
    "ng-packagr": "^5.4.0",
    "prettier": "^1.13.5",
    "protractor": "^6.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "~5.0.1",
    "tsickle": "^0.37.0",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~3.5.3",
    "yarn": "^1.17.3"
  }
}
